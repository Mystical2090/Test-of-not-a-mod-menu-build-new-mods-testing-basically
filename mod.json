{
    "geode": "4.4.0",
    "gd": {
        "mac": "2.2074",
        "win": "2.2074",
        "android": "2.2074",
        "ios": "2.2074"
    },
    "version": "v1.0.0",
    "id": "mystical.gtxripmxt.sp67.polo",
    "name": "Polo",
     "developers": ["Mystical","Gtxripmxt", "speedyfriend67"],
    "description": "A awesome mod menu made by me Gtxripmxt and speedyfriend67!",
    "links": {
            "source": "https://github.com/Gtxripmxt/not-a-mod-menu",
            "community": "https://discord.gg/gMRKR9m7KK"
    },
    "tags": ["customization", "gameplay", "cheat"],
    "settings": {
        "player": {
            "type": "title",
            "name": "Player"
        },
                "enable-noclip": {
                    "name": "Noclip",
                    "type": "bool",
                    "description": "Allows you to pass through objects.",
                    "default": false
                },
        
                "ignore-inputs": {
                    "name": "Ignore Inputs",
                    "type": "bool",
                    "description": "Ignores the inputs you've made.",
                    "default": false
                },
                "enable-jump-hack": {
                    "name": "Jump Hack",
                    "type": "bool",
                    "description": "Allows you to jump infinitely.",
                    "default": false
                },
        "Misc": {
            "type": "title",
            "name": "Misc"
        },
                "speedhack-bool": {
                    "name": "Speed Hack",
                    "type": "bool",
                    "description": "Allows you to speed up the game.",
                    "default": false
                },
                "speedhack": {
                    "name": "SpeedHack",
                    "type": "float",
                    "min": 0.01,
                    "max": 5.0,
                    "default": 1.0,
                    "enable-if": "speedhack-bool",
                    "controls": {
                "arrows": true,
                "arrow-step": 0.1,
                "big-arrows": false,
                "big-arrow-step": 0.5,
                "slider": true,
                "slider-step": 0.1,
                "input": false
                    }
                },
                "practice-music": {
                "name": "Practice Music Hack",
                "type": "bool",
                "description": "Bypasses practice music checkmark in settings.",
                "default": false
                },
                "layout-mode": {
                    "name": "Layout Mode",
                    "type": "bool",
                    "description": "Shows the layout of the level.",
                    "default": false
                },
                "icon-hack": {
                    "name": "Icon Hack",
                    "type": "bool",
                    "description": "Unlocks all the icons.",
                    "default": false
                },
                "color-hack": {
                    "name": "Color Hack",
                    "type": "bool",
                    "description": "Unlocks all the colors.",
                    "default": false
                },
                "instant-respawn": {
                    "name": "Suicide",
                    "type": "bool",
                    "description": "Instant Kills you. WARNING: Dont use noclip with this mod.",
                    "default": false
                },
                "show-hitboxes": {
                    "name": "test show hitboxes",
                    "type": "bool",
                    "description": "test hacks.",
                    "default": false
                },
                "show-hitboxes-on-death": {
                    "name": "show hitbox kn detah test",
                    "type": "bool",
                    "description": "yes yes",
                    "default": false
                },
        "creator": {
            "type": "title",
            "name": "Creator"
        },   
            "verify-hack": {
                "name": "Verify Hack",
                "type": "bool",
                "description": "Allows you to upload any level.",
                "default": false
                },
                "copy-hack-balls": {
                    "name": "Copy Hack",
                    "type": "bool",
                    "description": "Allows you to copy any level.",
                    "default": false
                },
            "custum-obj-bypass": {
                "name": "Custom Objects Bypass",
                "type": "bool",
                "description": "custom.objects.bypass.info.desc",
                "default": false
                }
        }
}
